{
  "name": "animated",
  "version": "0.2.1",
  "description": "Declarative Animations Library for React and React Native",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build:lib",
    "build": "npm run build:lib && npm run build:interactive-docs",
